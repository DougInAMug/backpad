//  https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/@prisma-generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// useless table for initialization
model Init {
  id      Int     @id @default(autoincrement())
  content String?
}
